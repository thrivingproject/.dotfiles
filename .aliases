# Python
alias py="python"
alias venv="python -m venv .venv && source .venv/bin/activate"
alias pybuild="rm -rf build dist && python setup.py py2app -A"
alias pytree="tree -I '__pycache__|dist|build|.git|.DS_Store|.venv' -a"

# Django
alias django='django-admin startproject'
alias djserv='python3 manage.py runserver'
alias djmake='python3 manage.py makemigrations'
alias djmigrate='python3 manage.py migrate'

# Other
alias aliases="code ~/.aliases"
alias zprofile="code ~/.zprofile"
alias ls="ls -G"
alias path="echo $PATH | tr ':' '\n'"
alias zshrc="code ~/.zshrc"
alias variables="printenv"
alias ghd="gh repo delete"
